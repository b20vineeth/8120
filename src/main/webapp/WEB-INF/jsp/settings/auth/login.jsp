<title>SB Admin 2 - Bootstrap Admin Theme</title>



<!-- Custom Fonts -->
<link
	href='/vendor/resources/vendor/font-awesome/css/font-awesome.min.css'
	rel='stylesheet' type='text/css'>
<!-- Custom CSS -->
<link href='/vendor/resources/dist/css/sb-admin-2.css' rel='stylesheet'>
<link href='/vendor/resources/vendor/style.css' rel='stylesheet'>
<!-- MetisMenu CSS -->
